{
  "compilerOptions": {
    "module": "Node16",
    "moduleResolution": "Node16",
    "target": "ES6",
    "lib": [
      "es2015",
      "dom"
    ],
    "jsx": "react",
    "outDir": "./static",
    "strict": true,
    "noFallthroughCasesInSwitch": true,
    "sourceMap": true
  },
  "exclude": [
    "static",
    "node_modules"
  ]
}
